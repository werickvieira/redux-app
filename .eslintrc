{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": [1, { "variables": true }],
    "no-continue": [1],
    "no-param-reassign": [2, {"props": false}],
    "comma-dangle": ["error", "always-multiline", {
      "functions": "never"
    }],
    "function-paren-newline": ["error", "consistent"]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals": {
  },
  "plugins": [
    "react"
  ]
}
